  "description": "A toolset for receiving, dispatching and store AIVDM/AIVDO Protocol data.",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "ts-mocha \"test/**/*.ts\" --recursive --exit",
