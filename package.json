  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3epnm/ais-tools/issues"
  },
  "homepage": "https://github.com/3epnm/ais-tools#readme",
  "dependencies": {
    "events": "^3.1.0",
    "geographiclib": "^1.50.0",
    "moment": "^2.24.0",
    "mongodb": "^3.5.5",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/mocha": "^7.0.2",
    "@types/mongodb": "^3.5.4",
    "@types/node": "^13.11.0",
    "mocha": "^7.1.1",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib/**/*"
